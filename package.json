{
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "version": "100.0.0",
  "description": "A cookie-based session middleware for Express.",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --watchAll",
    "test:debug": "DEBUG=multi-cookie-session npm run test:watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "connect",
    "express",
    "middleware",
    "session",
    "cookie",
    "cookies"
  ],
  "name": "multi-cookie-session",
  "author": "Donavon West",
  "module": "dist/multi-cookie-session.esm.js",
  "size-limit": [
    {
      "path": "dist/multi-cookie-session.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/multi-cookie-session.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/connect": "^3.4.34",
    "@types/cookies": "^0.7.6",
    "@types/debug": "^4.1.5",
    "@types/on-headers": "^1.0.0",
    "@types/supertest": "^2.0.10",
    "connect": "^3.7.0",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "supertest": "^6.1.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cookies": "^0.9.0",
    "on-headers": "^1.0.2"
  }
}
